<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.medicalsysbackend.mapper.PhysicalConditionMapper">

    <resultMap id="BaseResultMap" type="com.example.medicalsysbackend.domain.PhysicalCondition">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="user_id" column="user_id" jdbcType="INTEGER"/>
            <result property="checkup_date" column="checkup_date" jdbcType="DATE"/>
            <result property="height" column="height" jdbcType="DECIMAL"/>
            <result property="weight" column="weight" jdbcType="DECIMAL"/>
            <result property="blood_pressure" column="blood_pressure" jdbcType="VARCHAR"/>
            <result property="heart_rate" column="heart_rate" jdbcType="INTEGER"/>
            <result property="body_temperature" column="body_temperature" jdbcType="DECIMAL"/>
            <result property="symptoms" column="symptoms" jdbcType="VARCHAR"/>
            <result property="diagnosis" column="diagnosis" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,user_id,checkup_date,
        height,weight,blood_pressure,
        heart_rate,body_temperature,symptoms,
        diagnosis
    </sql>
</mapper>
