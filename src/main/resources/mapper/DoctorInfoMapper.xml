<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.medicalsysbackend.mapper.DoctorInfoMapper">

    <resultMap id="BaseResultMap" type="com.example.medicalsysbackend.domain.DoctorInfo">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="doctor_name" column="doctor_name" jdbcType="VARCHAR"/>
            <result property="specialty" column="specialty" jdbcType="VARCHAR"/>
            <result property="hospital_name" column="hospital_name" jdbcType="VARCHAR"/>
            <result property="department" column="department" jdbcType="VARCHAR"/>
            <result property="position" column="position" jdbcType="VARCHAR"/>
            <result property="experience_years" column="experience_years" jdbcType="INTEGER"/>
            <result property="introduction" column="introduction" jdbcType="VARCHAR"/>
            <result property="role" column="role" jdbcType="INTEGER"/>
            <result property="is_delete" column="is_delete" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,doctor_name,specialty,
        hospital_name,department,position,
        experience_years,introduction,role,
        is_delete
    </sql>
</mapper>
